{
  "name": "pet-helper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:adb": "adb reverse tcp:8081 tcp:8081 && yarn start",
    "android-debug": "npx react-native run-android && exit 0",
    "android-debug-clean": "yarn android-clean && yarn android-debug",
    "android-debug-fresh": "yarn android-fresh && yarn android-debug-clean",
    "android-debug-install": "yarn android-install-on-all android/app/build/outputs/apk/debug/app-debug.apk",
    "android-staging": "yarn jetify && cd android && ./gradlew assembleReleaseStaging && cd .. && yarn android-staging-install",
    "android-staging-clean": "yarn android-clean && yarn android-staging",
    "android-staging-fresh": "yarn android-fresh && yarn android-staging-clean",
    "android-staging-install": "yarn android-install-on-all android/app/build/outputs/apk/releaseStaging/app-releaseStaging.apk",
    "android-release": "yarn android-fresh && yarn jetify && cd android && ./gradlew assembleRelease && cd .. && yarn android-release-install",
    "android-release-bundle": "yarn android-fresh && yarn jetify && cd android && ./gradlew bundleRelease && cd ..",
    "android-release-install": "yarn android-install-on-all android/app/build/outputs/apk/release/app-release.apk",
    "android-clean": "cd android && ./gradlew clean",
    "android-fresh": "yarn refresh",
    "android-install-on-all": "adb devices | tail -n +2 | cut -sf 1 | xargs -iX adb -s X install -r",
    "ios": "react-native run-ios",
    "refresh": "rm -rf node_modules && yarn",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "sh ./.patch/script-patch.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.13.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.11.3",
    "@react-navigation/native": "5.9.2",
    "@react-navigation/stack": "5.14.1",
    "@types/ramda": "0.27.34",
    "@types/react-native-material-textfield": "0.16.3",
    "@types/react-native-snap-carousel": "3.8.2",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^8.3.1",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-thunk": "^2.1.0",
    "axios": "0.21.1",
    "date-fns": "2.16.1",
    "effector": "21.7.5",
    "effector-react": "21.2.1",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-fast-image": "8.3.4",
    "react-native-gesture-handler": "1.9.0",
    "react-native-keyboard-aware-scroll-view": "0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-material-textfield": "0.16.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.15.2",
    "react-native-snap-carousel": "3.9.1",
    "react-native-svg": "12.1.0",
    "react-native-vector-icons": "7.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-form": "^8.3.7",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.0",
    "@types/react-native": "0.63.45",
    "@types/react-test-renderer": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.2.1",
    "react-test-renderer": "16.13.1",
